<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    app:ignore="NamespaceTypo">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimens_110dp"
            android:scaleType="fitXY"
            android:src="@mipmap/dialog_vip_wake_top" />

        <TextView
            android:id="@+id/dialog_updateapp_version"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/white"
            android:gravity="center"
            android:layout_marginTop="-38dp"
            android:paddingTop="@dimen/dimens_20dp"
            android:text="@string/vip_wake_tittle"
            android:textColor="@color/color_282727"
            android:textSize="14sp" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/imageView"
            android:background="@drawable/shape_sol_ffffff_cor_bottom_16"
            android:orientation="vertical">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dimens_15dp"
                android:paddingLeft="@dimen/dimens_15dp"
                android:text="@string/vip_wake_miss"
                android:textColor="@color/updategray"
                android:textSize="@dimen/dimens_12dp" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
               android:padding="@dimen/dimens_15dp"
                android:orientation="vertical">


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dimens_40dp"
                    android:paddingBottom="@dimen/dimens_10dp"
                    android:orientation="horizontal">

                    <ImageView
                        android:src="@mipmap/dialog_vip_wake_free"
                        android:layout_width="@dimen/dimens_30dp"
                        android:layout_height="@dimen/dimens_30dp" />

                    <TextView
                        android:layout_marginLeft="@dimen/dimens_15dp"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:text="@string/vip_wake_free"
                        android:textColor="@color/color_4c4c4c" />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dimens_40dp"
                    android:paddingBottom="@dimen/dimens_10dp"
                    android:orientation="horizontal">

                    <ImageView
                        android:src="@mipmap/dialog_vip_wake_boyin"
                        android:layout_width="@dimen/dimens_30dp"
                        android:layout_height="@dimen/dimens_30dp" />

                    <TextView
                        android:layout_marginLeft="@dimen/dimens_15dp"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:text="@string/vip_wake_boyin"
                        android:textColor="@color/color_4c4c4c" />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dimens_40dp"
                    android:paddingBottom="@dimen/dimens_10dp"
                    android:orientation="horizontal">

                    <ImageView
                        android:src="@mipmap/dialog_vip_wake_qiang"
                        android:layout_width="@dimen/dimens_30dp"
                        android:layout_height="@dimen/dimens_30dp" />

                    <TextView
                        android:layout_marginLeft="@dimen/dimens_15dp"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:text="@string/vip_wake_qiang"
                        android:textColor="@color/color_4c4c4c" />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dimens_40dp"
                    android:paddingBottom="@dimen/dimens_10dp"
                    android:orientation="horizontal">

                    <ImageView
                        android:src="@mipmap/dialog_vip_wake_noad"
                        android:layout_width="@dimen/dimens_30dp"
                        android:layout_height="@dimen/dimens_30dp" />

                    <TextView
                        android:layout_marginLeft="@dimen/dimens_15dp"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:text="@string/vip_wake_noad"
                        android:textColor="@color/color_4c4c4c" />
                </LinearLayout>
                <TextView
                    android:id="@+id/dialog_continue"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dimens_38dp"
                    android:background="@drawable/shape_ff8350_20"
                    android:layout_marginBottom="@dimen/dimens_10dp"
                    android:layout_marginTop="@dimen/dimens_10dp"
                    android:gravity="center"
                    android:text="@string/vip_wake_continue"
                    android:textColor="@color/white" />
            </LinearLayout>
        </LinearLayout>

        <ImageView
            android:id="@+id/img_close"
            android:layout_width="@dimen/dimens_25dp"
            android:layout_height="@dimen/dimens_25dp"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="@dimen/dimens_25dp"
            android:src="@mipmap/close_red" />
    </LinearLayout>
</RelativeLayout>